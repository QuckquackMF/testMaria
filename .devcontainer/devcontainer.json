{
  "name": "MariaDB Devcontainer with Node and Python",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [3306],
  "remoteUser": "vscode",
  "postStartCommand": "python3 /workspace/create_db.py",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-mssql.mssql",
        "ms-python.python",
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}
