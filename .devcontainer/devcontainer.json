{
  "name": "MariaDB Devcontainer with Node and Python",
  "dockerComposeFile": "docker-compose.yml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "forwardPorts": [3306],
  "remoteUser": "vscode",
  "postStartCommand": [
    "python3 /workspace/.devcontainer/create_db.py"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-mssql.mssql",
        "ms-python.python",
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}
